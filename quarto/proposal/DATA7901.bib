@incollection{thompsonTheoreticalModelsGalaxy2020,
  title = {Theoretical {{Models}} of {{Galaxy Formation}} -- {{East}} versus {{West}}},
  booktitle = {The {{Discovery}} of {{Cosmic Voids}}},
