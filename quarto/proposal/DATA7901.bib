@article{cardamoneGalaxyZooGreen2009,
  title = {Galaxy {{Zoo Green Peas}}: {{Discovery}} of {{A Class}} of {{Compact Extremely Star-Forming Galaxies}}},
  shorttitle = {Galaxy {{Zoo Green Peas}}},
  author = {Cardamone, Carolin N. and Schawinski, Kevin and Sarzi, Marc and Bamford, Steven P. and Bennert, Nicola and Urry, C. M. and Lintott, Chris and Keel, William C. and Parejko, John and Nichol, Robert C. and Thomas, Daniel and Andreescu, Dan and Murray, Phil and Raddick, M. Jordan and Slosar, Anze and Szalay, Alex and VandenBerg, Jan},
  year = {2009},
  month = nov,
  journal = {Monthly Notices of the Royal Astronomical Society},
  volume = {399},
  number = {3},
  eprint = {0907.4155},
  primaryclass = {astro-ph},
  pages = {1191--1205},
  issn = {00358711, 13652966},
  doi = {10.1111/j.1365-2966.2009.15383.x},
  urldate = {2025-08-08},
  abstract = {We investigate a class of rapidly growing emission line galaxies, known as "Green Peas", first noted by volunteers in the Galaxy Zoo project because of their peculiar bright green colour and small size, unresolved in SDSS imaging. Their appearance is due to very strong optical emission lines, namely [O III] 5007 A, with an unusually large equivalent width of up to {\textasciitilde}1000 A. We discuss a well-defined sample of 251 colour-selected objects, most of which are strongly star forming, although there are some AGN interlopers including 8 newly discovered narrow Line Seyfert 1 galaxies. The star-forming Peas are low mass galaxies (M{\textasciitilde}10{\textasciicircum}8.5 - 10{\textasciicircum}10 M\_sun) with high star formation rates ({\textasciitilde}10 M\_sun/yr), low metallicities (log[O/H] + 12 {\textasciitilde} 8.7) and low reddening (E(B-V) {$<$} 0.25) and they reside in low density environments. They have some of the highest specific star formation rates (up to {\textasciitilde}10{\textasciicircum}\{-8\} yr{\textasciicircum}\{-1\}) seen in the local Universe, yielding doubling times for their stellar mass of hundreds of Myrs. The few star-forming Peas with HST imaging appear to have several clumps of bright star-forming regions and low surface density features that may indicate recent or ongoing mergers. The Peas are similar in size, mass, luminosity and metallicity to Luminous Blue Compact Galaxies. They are also similar to high redshift UV-luminous galaxies, e.g., Lyman-break galaxies and Lyman-alpha emitters, and therefore provide a local laboratory with which to study the extreme star formation processes that occur in high-redshift galaxies. Studying starbursting galaxies as a function of redshift is essential to understanding the build up of stellar mass in the Universe.},
  archiveprefix = {arXiv},
  keywords = {Astrophysics - Astrophysics of Galaxies,Astrophysics - Cosmology and Nongalactic Astrophysics},
  file = {/Users/greg/Zotero/storage/DJRK7MQG/Cardamone et al. - 2009 - Galaxy Zoo Green Peas Discovery of A Class of Compact Extremely Star-Forming Galaxies.pdf;/Users/greg/Zotero/storage/UNK7PNYN/0907.html}
}

@article{cavanaghMorphologicalClassificationGalaxies2021,
  title = {Morphological Classification of Galaxies with Deep Learning: Comparing 3-Way and 4-Way {{CNNs}}},
  shorttitle = {Morphological Classification of Galaxies with Deep Learning},
  author = {Cavanagh, Mitchell K. and Bekki, Kenji and Groves, Brent A.},
  year = {2021},
  month = jul,
  journal = {Monthly Notices of the Royal Astronomical Society},
  volume = {506},
  number = {1},
  eprint = {2106.01571},
  primaryclass = {astro-ph},
  pages = {659--676},
  issn = {0035-8711, 1365-2966},
  doi = {10.1093/mnras/stab1552},
  urldate = {2025-08-29},
  abstract = {Classifying the morphologies of galaxies is an important step in understanding their physical properties and evolutionary histories. The advent of large-scale surveys has hastened the need to develop techniques for automated morphological classification. We train and test several convolutional neural network architectures to classify the morphologies of galaxies in both a 3-class (elliptical, lenticular, spiral) and 4-class (+irregular/miscellaneous) schema with a dataset of 14034 visually-classified SDSS images. We develop a new CNN architecture that outperforms existing models in both 3 and 4-way classification, with overall classification accuracies of 83\% and 81\% respectively. We also compare the accuracies of 2-way / binary classifications between all four classes, showing that ellipticals and spirals are most easily distinguished ({$>$}98\% accuracy), while spirals and irregulars are hardest to differentiate (78\% accuracy). Through an analysis of all classified samples, we find tentative evidence that misclassifications are physically meaningful, with lenticulars misclassified as ellipticals tending to be more massive, among other trends. We further combine our binary CNN classifiers to perform a hierarchical classification of samples, obtaining comparable accuracies (81\%) to the direct 3-class CNN, but considerably worse accuracies in the 4-way case (65\%). As an additional verification, we apply our networks to a small sample of Galaxy Zoo images, obtaining accuracies of 92\%, 82\% and 77\% for the binary, 3-way and 4-way classifications respectively.},
  archiveprefix = {arXiv},
  keywords = {Astrophysics - Astrophysics of Galaxies,Astrophysics - Instrumentation and Methods for Astrophysics},
  file = {/Users/greg/Zotero/storage/DDBKCUTR/Cavanagh et al. - 2021 - Morphological classification of galaxies with deep learning comparing 3-way and 4-way CNNs.pdf;/Users/greg/Zotero/storage/UFAHFPT2/2106.html}
}

